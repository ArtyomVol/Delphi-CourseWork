program Project1;

uses
  Vcl.Forms,
  SysUtils,
  UnitMain in 'UnitMain.pas' {Form1},
  DecomposingTimeProcess in 'DecomposingTimeProcess.pas',
  ComboBoxActions in 'ComboBoxActions.pas',
